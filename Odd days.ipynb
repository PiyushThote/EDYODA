   "source": [
    "a = (1,2,3,4,5,6,7,8,9)\n",
    "odd_days = 0\n",
    "even_days = 0\n",
    "for i in a:\n",
    "    if i % 2 == 0:\n",
    "        even_days += 1\n",
    "    else:\n",
    "        odd_days += 1\n",
    "print(\"Number of even numbers = \", even_days)\n",
    "print(\"Number of odd numbers = \", odd_days)\n",
    "print(\"End of program\".center(100,'*'))"
   ]
